            lp.LedCtrlRaw( 78 ,3 ,3 ,3)
            lp.LedCtrlRaw( 88 ,3 ,3 ,3)

        while 1:
            but = lp.ButtonStateRaw()
            if caseFirstAnime == 1:
                caseFirstAnime = 0
                caseAnime()

            def menuColorAnime():
                lp.ButtonFlush()
                flushAnime = 1
                while 1:

                    def slected1Anime():
                        lp.ButtonFlush()
                        flushAnime = 1
                        while 1:
                            but = lp.ButtonStateRaw()
                            if flushAnime == 1:
                                time.sleep(0.3)
                                lp.ButtonFlush()
                                for x in range (8):
                                    lp.LedCtrlPulseXYByCode( x, 1, 1 )
                                for x in range (8):
                                    lp.LedCtrlPulseXYByCode( x, 2, 1 )
                                for x in range (8):
                                    lp.LedCtrlPulseXYByCode( x, 3, 1 )
                                for x in range (8):
                                    lp.LedCtrlPulseXYByCode( x, 4, 1 )
                                for x in range (8):
                                    lp.LedCtrlPulseXYByCode( x, 5, 1 )
                                for x in range (8):
                                    lp.LedCtrlPulseXYByCode( x, 6, 1 )
                                for x in range (8):
                                    lp.LedCtrlPulseXYByCode( x, 7, 1 )
                                for x in range (8):
                                    lp.LedCtrlPulseXYByCode( x, 8, 1 )
                                flushAnime = 0
                            if but != []:
                                if but == [49, 127]:
                                    lp.Reset()
                                    lp.Close
                                    sys.exit()
                                else:
                                    reset()
                                    menuColorAnime()
                                    break


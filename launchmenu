    def mute():
        reset()
        os.system("C:/Users/Paul/Documents/SARAH/plugins/jarvis/nircmd/nircmdc.exe mutesysvolume 1")
        lp.LedCtrlRaw( 14 ,25 ,25 ,25)
        lp.LedCtrlRaw( 15 ,25 ,25 ,25)
        lp.LedCtrlRaw( 23 ,25 ,25 ,25)
        lp.LedCtrlRaw( 26 ,25 ,25 ,25)
        lp.LedCtrlRaw( 33 ,25 ,25 ,25)
        lp.LedCtrlRaw( 36 ,25 ,25 ,25)
        lp.LedCtrlRaw( 47 ,25 ,25 ,25)
        lp.LedCtrlRaw( 42 ,25 ,25 ,25)
        lp.LedCtrlRaw( 68 ,25 ,25 ,25)
        lp.LedCtrlRaw( 58 ,25 ,25 ,25)
        lp.LedCtrlRaw( 61 ,25 ,25 ,25)
        lp.LedCtrlRaw( 62 ,25 ,25 ,25)
        lp.LedCtrlRaw( 63 ,25 ,25 ,25)
        lp.LedCtrlRaw( 64 ,25 ,25 ,25)
        lp.LedCtrlRaw( 65 ,25 ,25 ,25)
        lp.LedCtrlRaw( 66 ,25 ,25 ,25)
        lp.LedCtrlRaw( 67 ,25 ,25 ,25)
        lp.LedCtrlRaw( 51 ,25 ,25 ,25)

        lp.LedCtrlRaw( 82 ,63 ,3 ,3)
        lp.LedCtrlRaw( 83 ,63 ,3 ,3)
        lp.LedCtrlRaw( 84 ,63 ,3 ,3)
        lp.LedCtrlRaw( 85 ,63 ,3 ,3)
        lp.LedCtrlRaw( 86 ,63 ,3 ,3)
        lp.LedCtrlRaw( 87 ,63 ,3 ,3)
        time.sleep(1)
        resetHome()

    def unMute():
        reset()
        os.system("C:/Users/Paul/Documents/SARAH/plugins/jarvis/nircmd/nircmdc.exe mutesysvolume 0")
        lp.LedCtrlRaw( 14 ,25 ,25 ,25)
        lp.LedCtrlRaw( 15 ,25 ,25 ,25)

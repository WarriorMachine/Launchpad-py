import launchpad_py as launchpad
import os
import sys
import time
import math
import random

#clear CMD
os.system("cls")

#definition instances etc..
mode = None
ARROW_UP = 1
ARROW_DOWN = 1
ARROW_LEFT = 1
ARROW_RIGHT = 1
SESSION = 1
USER_1 = 1
USER_2 = 1
MIXER = 1
VOLUME = 1
PAN = 1
SEND_A = 1
SEND_B = 1
STOP = 1
MUTE = 1
SOLO = 1
RECORD_ARM = 1

try:
    lp = launchpad.LaunchpadMk2()
    lp.Open(0,"Mk2")
    mode = "Mk2"
except mode is None:
    print("ERREUR : Aucun lauchpad trouv√©")
    time.sleep(10)

lp.Reset()
lp.ButtonFlush()

#case 1
lp.LedCtrlRaw( 11 ,3 ,3 ,3)
lp.LedCtrlRaw( 21 ,3 ,3 ,3)
lp.LedCtrlRaw( 12 ,3 ,3 ,3)
lp.LedCtrlRaw( 22 ,3 ,3 ,3)

#case 2
lp.LedCtrlRaw( 41 ,3 ,3 ,3)
lp.LedCtrlRaw( 51 ,3 ,3 ,3)
lp.LedCtrlRaw( 42 ,3 ,3 ,3)
lp.LedCtrlRaw( 52 ,3 ,3 ,3)

#anime
lp.LedCtrlRaw( 71 ,63 ,10 ,63)
lp.LedCtrlRaw( 81 ,63 ,20 ,63)
lp.LedCtrlRaw( 72 ,63 ,30 ,63)
lp.LedCtrlRaw( 82 ,63 ,35 ,63)

#case 4
lp.LedCtrlRaw( 14 ,3 ,3 ,3)
lp.LedCtrlRaw( 24 ,3 ,3 ,3)
lp.LedCtrlRaw( 15 ,3 ,3 ,3)
lp.LedCtrlRaw( 25 ,3 ,3 ,3)

#case 5
lp.LedCtrlRaw( 44 ,3 ,3 ,3)
lp.LedCtrlRaw( 54 ,3 ,3 ,3)
lp.LedCtrlRaw( 45 ,3 ,3 ,3)
lp.LedCtrlRaw( 55 ,3 ,3 ,3)

#case 6
lp.LedCtrlRaw( 74 ,3 ,3 ,3)
lp.LedCtrlRaw( 84 ,3 ,3 ,3)
lp.LedCtrlRaw( 75 ,3 ,3 ,3)
lp.LedCtrlRaw( 85 ,3 ,3 ,3)

#windows
lp.LedCtrlRaw( 17 ,63 ,0 ,0)
lp.LedCtrlRaw( 27 ,0 ,0 ,63)
lp.LedCtrlRaw( 18 ,0 ,63 ,0)
lp.LedCtrlRaw( 28 ,32 ,63 ,0)

#case 8
lp.LedCtrlRaw( 47 ,3 ,3 ,3)
lp.LedCtrlRaw( 57 ,3 ,3 ,3)
lp.LedCtrlRaw( 48 ,3 ,3 ,3)
lp.LedCtrlRaw( 58 ,3 ,3 ,3)

#case 9
lp.LedCtrlRaw( 77 ,3 ,3 ,3)
lp.LedCtrlRaw( 87 ,3 ,3 ,3)
lp.LedCtrlRaw( 78 ,3 ,3 ,3)
lp.LedCtrlRaw( 88 ,3 ,3 ,3)

#STOP
lp.LedCtrlRaw( 49, 30, 3, 2)

#MUTE
lp.LedCtrlRaw( 39 ,5 ,35 ,5)

#RECORD ARM
lp.LedCtrlRaw( 19, 63 ,32 ,10)

while 1:
    but = lp.ButtonStateRaw()

    def reset():
        for x in range(9):
                lp.LedCtrlRaw( 10+x, 0, 0, 0)
        for x in range(9):
                lp.LedCtrlRaw( 20+x, 0, 0, 0)
        for x in range(9):
                lp.LedCtrlRaw( 30+x, 0, 0, 0)
        for x in range(9):
                lp.LedCtrlRaw( 40+x, 0, 0, 0)
        for x in range(9):
                lp.LedCtrlRaw( 50+x, 0, 0, 0)
        for x in range(9):
                lp.LedCtrlRaw( 60+x, 0, 0, 0)
        for x in range(9):
                lp.LedCtrlRaw( 70+x, 0, 0, 0)
        for x in range(9):
                lp.LedCtrlRaw( 80+x, 0, 0, 0)

    def resetHome():
        #case 1
        lp.LedCtrlRaw( 11 ,3 ,3 ,3)
        lp.LedCtrlRaw( 21 ,3 ,3 ,3)
        lp.LedCtrlRaw( 12 ,3 ,3 ,3)
        lp.LedCtrlRaw( 22 ,3 ,3 ,3)
        #case 2
        lp.LedCtrlRaw( 41 ,3 ,3 ,3)
        lp.LedCtrlRaw( 51 ,3 ,3 ,3)

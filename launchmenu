        lp.LedCtrlRaw( 85 ,3 ,3 ,3)
        #windows
        lp.LedCtrlRaw( 17 ,63 ,0 ,0)
        lp.LedCtrlRaw( 27 ,0 ,0 ,63)
        lp.LedCtrlRaw( 18 ,0 ,63 ,0)
        lp.LedCtrlRaw( 28 ,32 ,63 ,0)
        #case 8
        lp.LedCtrlRaw( 47 ,3 ,3 ,3)
        lp.LedCtrlRaw( 57 ,3 ,3 ,3)
        lp.LedCtrlRaw( 48 ,3 ,3 ,3)
        lp.LedCtrlRaw( 58 ,3 ,3 ,3)
        #case 9
        lp.LedCtrlRaw( 77 ,3 ,3 ,3)
        lp.LedCtrlRaw( 87 ,3 ,3 ,3)
        lp.LedCtrlRaw( 78 ,3 ,3 ,3)
        lp.LedCtrlRaw( 88 ,3 ,3 ,3)
        #vide
        for i in range(9):
            lp.LedCtrlRaw( 30+i ,0 ,0 ,0)
        for i in range(9):
            lp.LedCtrlRaw( 60+i ,0 ,0 ,0)
        lp.LedCtrlRaw( 13 ,0 ,0 ,0)
        lp.LedCtrlRaw( 23 ,0 ,0 ,0)
        lp.LedCtrlRaw( 43 ,0 ,0 ,0)
        lp.LedCtrlRaw( 53 ,0 ,0 ,0)
        lp.LedCtrlRaw( 73 ,0 ,0 ,0)
        lp.LedCtrlRaw( 83 ,0 ,0 ,0)
        lp.LedCtrlRaw( 16 ,0 ,0 ,0)
        lp.LedCtrlRaw( 26 ,0 ,0 ,0)
        lp.LedCtrlRaw( 46 ,0 ,0 ,0)
        lp.LedCtrlRaw( 56 ,0 ,0 ,0)
        lp.LedCtrlRaw( 76 ,0 ,0 ,0)
        lp.LedCtrlRaw( 86 ,0 ,0 ,0)

    def animationMenu():
        caseFirstAnime = 1
        reset()

        def caseAnime():
            #Case animation 1
            lp.LedCtrlPulseXYByCode( 0 , 7, random.randint(1, 60))
            lp.LedCtrlPulseXYByCode( 1 , 7, random.randint(1, 60))
            lp.LedCtrlPulseXYByCode( 0 ,8 ,random.randint(1, 60))
            lp.LedCtrlPulseXYByCode( 1 ,8 ,random.randint(1, 60))
            #Case animation 2
            lp.LedCtrlRaw( 41 ,3 ,3 ,3)
            lp.LedCtrlRaw( 51 ,3 ,3 ,3)
            lp.LedCtrlRaw( 42 ,3 ,3 ,3)
            lp.LedCtrlRaw( 52 ,3 ,3 ,3)
            #Case animation 3
            lp.LedCtrlRaw( 71 ,3 ,3 ,3)
            lp.LedCtrlRaw( 81 ,3 ,3 ,3)
            lp.LedCtrlRaw( 72 ,3 ,3 ,3)
            lp.LedCtrlRaw( 82 ,3 ,3 ,3)
            #Case animation 4
            lp.LedCtrlRaw( 14 ,3 ,3 ,3)
            lp.LedCtrlRaw( 24 ,3 ,3 ,3)
            lp.LedCtrlRaw( 15 ,3 ,3 ,3)
            lp.LedCtrlRaw( 25 ,3 ,3 ,3)
